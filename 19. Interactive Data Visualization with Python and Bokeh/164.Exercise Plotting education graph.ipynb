   "source": [
    "from bokeh.plotting import figure\n",
    "from bokeh.io import output_file , show\n",
    "import pandas\n",
    "\n",
    "df = pandas.read_csv(\"bachelors.csv\")\n",
    "\n",
    "x = df[\"Year\"]\n",
    "y = df[\"Engineering\"]\n",
    "\n",
    "output_file (\"Education_Graph.html\")\n",
    "f = figure()\n",
    "\n",
    "f.line(x,y)\n",
    "\n",
    "show (f)"
   ]
